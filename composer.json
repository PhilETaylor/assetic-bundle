{
    "name": "philetaylor/assetic-bundle",
    "description": "Integrates Assetic into Symfony4",
    "keywords": ["assets", "compression", "minification"],
    "homepage": "https://github.com/sanpii/AsseticBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Kris Wallsmith",
            "email": "kris.wallsmith@gmail.com",
            "homepage": "http://kriswallsmith.net/"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "sanpi/assetic": "~1.5"
    },
    "require-dev": {
        "patchwork/jsqueeze": "~1.0",
        "kriswallsmith/spork": "~0.3",
        "phpunit/phpunit": "~7.0",
        "symfony/twig-bundle": "~2.3|~3.0|~4",
        "symfony/dom-crawler": "~2.3|~3.0|~4",
        "symfony/css-selector": "~2.3|~3.0|~4",
        "symfony/phpunit-bridge": "~2.7|~3.0|~4"
    },
    "replace": {
        "symfony/assetic-bundle": "self.version"
    },
    "conflict": {
        "twig/twig": "<1.27",
        "kriswallsmith/spork": "<=0.2"
    },
    "suggest": {
        "symfony/twig-bundle": "to use the Twig integration",
        "kriswallsmith/spork": "to be able to dump assets in parallel"
    },
    "autoload": {
        "psr-4": { "Symfony\\Bundle\\AsseticBundle\\": "" }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.8-dev"
        }
    }
}
